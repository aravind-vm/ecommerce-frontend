) : (\n              <ProductsPage />\n            )\n          }\n        />\n\n        <Route\n          path=\"/product/:id/items\"\n          element={\n            isFirsTimeInHomePage ? (\n              <Navigate to=\"/home\" replace />\n            ) : (\n              <ProductItemsPage />\n            )\n          }\n        />\n      </Routes>\n    </>\n  );\n}\n\nexport default App;\n","D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Components\\NavBar\\NavBar.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Pages\\HomePage.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Components\\Promotions\\Promotion.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Store\\store.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Components\\ProductCategory\\ProductCategoryList.js",["178"],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Components\\ProductCategory\\ProductCategoryItem.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Pages\\ProductsPage.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\UI\\Card\\Card.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Components\\ProductsList\\ProductList.js",["179","180"],"import React, { useEffect, useCallback } from \"react\";\r\nimport Card from \"../../UI/Card/Card\";\r\nimport classes from \"./ProductList.module.css\";\r\nimport Product from \"./Product\";\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\nimport { fetchProductListHandler } from \"../../Store/productList-slice\";\r\nimport { useParams } from \"react-router-dom\";\r\nimport { cartAction } from \"../../Store/cart-slice\";\r\n\r\nconst ProductList = () => {\r\n  const productCategoryList = useSelector((state) => state.pCategory.data);\r\n  const dispatch = useDispatch();\r\n  const params = useParams();\r\n  const categoryId = parseInt(params.id);\r\n  const category = productCategoryList.filter(\r\n    (item) => parseInt(item.id) === categoryId\r\n  );\r\n\r\n  const categoryName = category[0].categoryName;\r\n\r\n  useEffect(() => {\r\n    dispatch(fetchProductListHandler(categoryId));\r\n    dispatch(cartAction.clearskuOutOfStock());\r\n  }, [fetchProductListHandler]);\r\n\r\n  const productList = useSelector((state) => state.pList.data);\r\n\r\n  let productListTile;\r\n  if (!productList || productList.length === 0) {\r\n    productListTile = (\r\n      <h2>\r\n        <p>No product found</p>\r\n      </h2>\r\n    );\r\n  } else {\r\n    productListTile = productList.map((item) => {\r\n      return <Product productItem={item} key={item.id}></Product>;\r\n    });\r\n  }\r\n\r\n  return (\r\n    <>\r\n      {productList.length !== 0 && (\r\n        <div className={classes.title}>{categoryName}</div>\r\n      )}\r\n      <Card className={classes.pcList}>{productListTile}</Card>\r\n      {productList.length !== 0 && (\r\n        <div className={classes.button}>\r\n          <button> &lt;</button>\r\n          <button> &gt;</button>\r\n        </div>\r\n      )}\r\n    </>\r\n  );\r\n};\r\n\r\nexport default ProductList;\r\n","D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Components\\ProductsList\\Product.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Pages\\ProductItemsPage.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Components\\Sku\\SkuList.js",["181"],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Components\\Cart\\CartButton.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Store\\ui-slice.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Components\\Cart\\Cart.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\UI\\Modal\\Modal.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Components\\Cart\\CartItem.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Store\\productCategory-slice.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\Store\\productList-slice.js",[],"D:\\MyProjects\\ReactFrontEndEcommerce-real\\src\\UI\\LoadingSpinner\\LoadingSpinner.js",[],"D: